;
; BIND data file for local loopback interface
;
$TTL	12000
@	IN	SOA	ns1.denisenko.loc. masterhost.denisenko.loc. (
			4967		; Serial
			10000		; Refresh
			3600		; Retry
			500000		; Expire
			86400		; 1 day TTL neg
);
@		IN	NS	ns1.denisenko.loc.
ns1		IN	A	192.168.57.1
localhost	IN	A	127.0.0.1
balance		IN	A	192.168.57.1
